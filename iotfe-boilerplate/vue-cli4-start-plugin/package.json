{
  "name": "iotfe-vuecli4-boilerplate",
  "version": "0.1.0",
  "description": "A vue h5 template with Gree UI for greeplus plugin",
  "author": "iotfe",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service serve --mode test",
    "test:api": "cd ../static && npm start",
    "debug": "npm run build:xml && vue-cli-service build --mode debug && npm run build:zip",
    "build": "npm run build:xml && vue-cli-service build && npm run build:zip",
    "build:xml": "node config_xml.js",
    "build:zip": "node compress_zip.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "gree-ui": "http://10.2.11.226/iot-4/web/gree-ui.git",
    "vconsole": "^3.3.4",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.5",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.3",
    "@vue/cli-plugin-eslint": "~4.2.3",
    "@vue/cli-service": "~4.2.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "compressing": "^1.5.1",
    "copy-webpack-plugin": "^5.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "jsdom": "^16.4.0",
    "node-sass": "^4.13.1",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.0.3",
    "xml2js": "^0.4.23"
  }
}
